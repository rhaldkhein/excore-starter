{
  "name": "excore-app",
  "version": "0.0.0",
  "description": "A boilerplate for Excore project",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf build && babel app --out-dir build",
    "watch": "rm -rf build && babel app --watch --out-dir build",
    "server": "DEBUG=excore:* nodemon build",
    "dev": "npm run watch && nodemon build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhaldkhein/excore-app.git"
  },
  "keywords": [
    "boilderplate",
    "express",
    "jservice"
  ],
  "author": "RhaldKhein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhaldkhein/excore-app/issues"
  },
  "homepage": "https://github.com/rhaldkhein/excore-app#readme",
  "dependencies": {
    "excore": "0.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "nodemon": "^1.19.1"
  }
}